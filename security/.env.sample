# YucaMedia Website Environment Variables Sample
# Copy this file to .env and fill in the values

# Server Configuration
PORT=3000
NODE_ENV=development

# Solana Configuration
VITE_SOLANA_NETWORK=devnet
VITE_SOLANA_RPC_ENDPOINT=https://api.devnet.solana.com

# Contract Addresses - Replace with your deployed contract addresses
VITE_LOTTERY_PROGRAM_ID=YOUR_LOTTERY_PROGRAM_ID
VITE_STUDIO_PROGRAM_ID=YOUR_STUDIO_PROGRAM_ID

# API Keys - DO NOT commit actual API keys to version control
VITE_API_KEY=YOUR_API_KEY

# Security Settings
VITE_CSP_REPORT_URI=https://example.com/csp-report
